<div id="background-carousel">
    <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
        <div class="row">
            <div class="carousel-inner">
                <div class="carousel-item active">
                <img class="img-responsive" src="https://mdbootstrap.com/img/Photos/Slides/img%20(17).jpg"
                    alt="First slide">
                </div>
                <div class="carousel-item">
                <img class="img-responsive" src="https://mdbootstrap.com/img/Photos/Slides/img%20(16).jpg"
                    alt="Second slide">
                </div>
                <div class="carousel-item">
                <img class="img-responsive" src="https://mdbootstrap.com/img/Photos/Slides/img%20(23).jpg"
                    alt="Third slide">
                </div>
            </div>
        </div>    
        <div class="row">
            <div class="col-md-12 products_card-container">
                <div class="col-sm-9">
                    <%= render "./catalog/card-products"%>
                </div>
            </div>
            <div class="col-md-12">
                <%= render "footer" %>
            </div>
        </div> 
    </div>
</div> 